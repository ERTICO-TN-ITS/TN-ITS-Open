@prefix : <http://spec.tn-its.eu/owl/tnits-owl#> .
@prefix lr: <http://www.roadotl.eu/iso19148/def/> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix gsp: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://spec.tn-its.eu/owl/tnits-owl> a owl:Ontology ;
	owl:imports <http://www.w3.org/2004/02/skos/core> ;
	owl:imports gsp: ;
	owl:imports lr: ;
	owl:imports <http://def.isotc211.org/iso19115/-1/2014/MetadataInformation> ;
	owl:imports <http://spec.tn-its.eu/codelists/allcodes> ;

	### GeoSPARQL rdf file: http://schemas.opengis.net/geosparql/1.0/geosparql_vocab_all.rdf
	### ISO 19148 ttl file: https://ontologi.atlas.vegvesen.no/nvdb/core/iso-19148.ttl
	### ISO 19115 rdf file: http://def.isotc211.org/iso19115/-1/2014/MetadataInformation.rdf

	dc:creator "TN-ITS" ;
	dc:date "08.06.2022" ;
	dc:description "Ontology for the TN-ITS model for exchange of changes in road attributes"@en ;
	dc:title "TN-ITS"@en ;
	dc:abstract "Ontology for exchange of changes in road attributes" .

### http://spec.tn-its.eu/owl/tnits-owl#tnits
:tnits a owl:Class ;
         owl:disjointUnionOf ( :tnitsFeature :tnitsCodeList :tnitsEnumeration :tnitsDataType ) ;
         rdfs:label "TN-ITS Root"@en .
### http://spec.tn-its.eu/owl/tnits-owl#Feature
:tnitsFeature a owl:Class ;
         rdfs:subClassOf <http://def.isotc211.org/iso19109/2015/GeneralFeatureModel#AnyFeature> ,
                          :tnits ,
                       gsp:Feature ;
         rdfs:label "TN-ITS Feature"@en .
### http://spec.tn-its.eu/owl/tnits-owl#CodeList
:tnitsCodeList a owl:Class ;
         rdfs:subClassOf :tnits, skos:Concept ;
         rdfs:label "TN-ITS Code value"@en .
### http://spec.tn-its.eu/owl/tnits-owl#Enumeration
:tnitsEnumeration a owl:Class ;
         rdfs:subClassOf :tnits, skos:Concept ;
         rdfs:label "TN-ITS Enumeration value"@en .
### http://spec.tn-its.eu/owl/tnits-owl#DataType
:tnitsDataType a owl:Class ;
         rdfs:subClassOf :tnits ;
         rdfs:label "TN-ITS Data type"@en .



### http://spec.tn-its.eu/owl/tnits-owl:Identifier
:Identifier a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "External unique object identifier published by the responsible body, which may be used by external applications to reference the spatial object."@en ;
       rdfs:label "Identifier"@en .

### http://spec.tn-its.eu/owl/tnits-owl#localId
:localId rdf:type owl:DatatypeProperty ;
         skos:definition "A local identifier, assigned by the data provider. The local identifier is unique within the namespace, that is no other spatial object carries the same unique identifier. "@en ;
         rdfs:domain :Identifier;
         rdfs:range xsd:string;
         rdf:type owl:FunctionalProperty;
         rdfs:label "localId"@en .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :localId;
         owl:onDataRange xsd:string;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :localId;
         owl:allValuesFrom xsd:string;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#namespace
:namespace rdf:type owl:DatatypeProperty ;
         skos:definition "Namespace uniquely identifying the data source of the spatial object."@en ;
         rdfs:domain :Identifier;
         rdfs:range xsd:string;
         rdf:type owl:FunctionalProperty;
         rdfs:label "namespace"@en .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :namespace;
         owl:onDataRange xsd:string;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :namespace;
         owl:allValuesFrom xsd:string;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#versionId
:versionId rdf:type owl:DatatypeProperty ;
         skos:definition "The identifier of the particular version of the spatial object, with a maximum length of 25 characters. If the specification of a spatial object type with an external object identifier includes life-cycle information, the version identifier is used to distinguish between the different versions of a spatial object. Within the set of all versions of a spatial object, the version identifier is unique. "@en ;
         rdfs:domain :Identifier;
         rdfs:range xsd:string;
         rdfs:label "versionId"@en .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :versionId;
         owl:onDataRange xsd:string;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:Identifier rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :versionId;
         owl:allValuesFrom xsd:string;
         ] .



### http://spec.tn-its.eu/owl/tnits-owl:Dataset
:Dataset a owl:Class ;
       rdfs:subClassOf :tnitsFeature ;
         skos:definition "Abstract class that represents any type of dataset that may be exchanged using TN-ITS  "@en ;
       rdfs:label "Dataset"@en .

### http://spec.tn-its.eu/owl/tnits-owl#Dataset.metadata
:Dataset.metadata rdf:type owl:ObjectProperty ;
         skos:definition "Metadata for the dataset. "@en ;
         rdfs:domain :Dataset;
         rdfs:range :Metadata;
         rdfs:label "Dataset.metadata"@en .
:Dataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :Dataset.metadata;
         owl:onClass :Metadata;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:Dataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :Dataset.metadata;
         owl:allValuesFrom :Metadata;
         ] .

:Dataset owl:disjointUnionOf ( :RoadFeatureDataset :FeedbackDataset  ) ; .

### http://spec.tn-its.eu/owl/tnits-owl:FeedbackDataset
:FeedbackDataset a owl:Class ;
         skos:definition "A dataset containing feedback to the road authorities/road operators from an information provider/map provider in response to data download and integration  "@en ;
       rdfs:label "FeedbackDataset"@en .
:FeedbackDataset rdfs:subClassOf :Dataset .

### http://spec.tn-its.eu/owl/tnits-owl#feedbackInfo
:feedbackInfo rdf:type owl:ObjectProperty ;
         skos:definition "The feedback information  "@en ;
         rdfs:domain :FeedbackDataset;
         rdfs:range :FeedbackInformation;
         rdf:type owl:FunctionalProperty;
         rdfs:label "feedbackInfo"@en .
:FeedbackDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :feedbackInfo;
         owl:onClass :FeedbackInformation;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:FeedbackDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :feedbackInfo;
         owl:allValuesFrom :FeedbackInformation;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl:RoadFeatureDataset
:RoadFeatureDataset a owl:Class ;
         skos:definition "A dataset containing road feature with optional update information.  "@en ;
       rdfs:label "RoadFeatureDataset"@en .

### http://spec.tn-its.eu/owl/tnits-owl#roadFeatures
:roadFeatures rdf:type owl:ObjectProperty ;
         skos:definition "The set of RoadFeatures contained in this dataset "@en ;
         rdfs:domain :RoadFeatureDataset;
         rdfs:range :RoadFeature;
         rdfs:label "roadFeatures"@en .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :roadFeatures;
         owl:onClass :RoadFeature;
       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :roadFeatures;
         owl:allValuesFrom :RoadFeature;
         ] .

:RoadFeatureDataset rdfs:subClassOf :Dataset .

### http://spec.tn-its.eu/owl/tnits-owl#RoadFeatureDataset.type
:RoadFeatureDataset.type rdf:type owl:ObjectProperty ;
         skos:definition "Specifies the type of dataset "@en ;
         rdfs:domain :RoadFeatureDataset;
         rdfs:range :DatasetType;
         rdf:type owl:FunctionalProperty;
         rdfs:label "RoadFeatureDataset.type"@en .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeatureDataset.type;
         owl:onClass :DatasetType;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeatureDataset.type;
         owl:allValuesFrom :DatasetType;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#defaultLRM
:defaultLRM rdf:type owl:ObjectProperty ;
         skos:definition "Default linear referencing method to be used in the dataset."@en ;
         rdfs:domain :RoadFeatureDataset;
         rdfs:range <http://www.roadotl.eu/iso19148/def/LinearReferencingMethod>;
         rdfs:label "defaultLRM"@en .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :defaultLRM;
         owl:onClass <http://www.roadotl.eu/iso19148/def/LinearReferencingMethod>;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeatureDataset rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :defaultLRM;
         owl:allValuesFrom <http://www.roadotl.eu/iso19148/def/LinearReferencingMethod>;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl:DatasetType
:DatasetType a owl:Class ;
       rdfs:subClassOf :tnitsEnumeration ;
         skos:definition "Specifies the different possible types of datasets for road features  "@en ;
       rdfs:label "DatasetType"@en .

### http://spec.tn-its.eu/owl/tnits-owl:DatasetTypeCode
:DatasetTypeCode a skos:ConceptScheme ;
         skos:definition "Specifies the different possible types of datasets for road features  "@en ;
       dc:isFormatOf :DatasetType .

### http://spec.tn-its.eu/owl/tnits-owl:DatasetType.Update
:DatasetType.Update a :DatasetType,skos:Concept ;
         skos:inScheme :DatasetTypeCode ;
         skos:definition "A dataset consisting of only incremental updates for road features "@en ;
         rdfs:label "Update"@no .

### http://spec.tn-its.eu/owl/tnits-owl:DatasetType.Snapshot
:DatasetType.Snapshot a :DatasetType,skos:Concept ;
         skos:inScheme :DatasetTypeCode ;
         skos:definition "A snapshot dataset contains information (current status) of all road features in an area, or of all occurrences of one or more specified types of road features"@en ;
         rdfs:label "Snapshot"@no .
:DatasetType owl:oneOf ( :DatasetType.Update :DatasetType.Snapshot  ) ; .


### http://spec.tn-its.eu/owl/tnits-owl:AbstractRoadFeatureProperty
:AbstractRoadFeatureProperty a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "An abstract generic (type independent) representation of a road feature property value "@en ;
       rdfs:label "AbstractRoadFeatureProperty"@en .
:AbstractRoadFeatureProperty owl:disjointUnionOf ( :GenericRoadFeatureProperty :RoadSignProperty  ) ; .

### http://spec.tn-its.eu/owl/tnits-owl:ChargingPointUsageType
:ChargingPointUsageType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the use of electric charging point "@en ;
       rdfs:label "ChargingPointUsageType"@en .

### http://spec.tn-its.eu/owl/tnits-owl:ChargingPointUsageTypeCode
:ChargingPointUsageTypeCode a skos:ConceptScheme ;
         skos:definition "Specifies the available list of enumerations for the use of electric charging point "@en ;
       dc:isFormatOf :ChargingPointUsageType .

### http://spec.tn-its.eu/owl/tnits-owl:GasType
:GasType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for types of propellant gas."@en ;
       rdfs:label "GasType"@en .

### http://spec.tn-its.eu/owl/tnits-owl:GasTypeCode
:GasTypeCode a skos:ConceptScheme ;
         skos:definition "Specifies the available list of enumerations for types of propellant gas."@en ;
       dc:isFormatOf :GasType .

### http://spec.tn-its.eu/owl/tnits-owl:GenericRoadFeatureProperty
:GenericRoadFeatureProperty a owl:Class ;
         skos:definition "A representation of a road feature property value "@en ;
       rdfs:label "GenericRoadFeatureProperty"@en .
:GenericRoadFeatureProperty rdfs:subClassOf :AbstractRoadFeatureProperty .

### http://spec.tn-its.eu/owl/tnits-owl#GenericRoadFeatureProperty.type
:GenericRoadFeatureProperty.type rdf:type owl:ObjectProperty ;
         skos:definition "The type of road feature property by reference to an item in an external code list"@en ;
         rdfs:domain :GenericRoadFeatureProperty;
         rdfs:range :RoadFeaturePropertyType;
         rdf:type owl:FunctionalProperty;
         rdfs:label "GenericRoadFeatureProperty.type"@en .
:GenericRoadFeatureProperty rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :GenericRoadFeatureProperty.type;
         owl:onClass :RoadFeaturePropertyType;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:GenericRoadFeatureProperty rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :GenericRoadFeatureProperty.type;
         owl:allValuesFrom :RoadFeaturePropertyType;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#GenericRoadFeatureProperty.value
:GenericRoadFeatureProperty.value rdf:type owl:ObjectProperty ;
         skos:definition "The value of the road feature property"@en ;
         rdfs:domain :GenericRoadFeatureProperty;
         rdfs:range :Any;
         rdfs:label "GenericRoadFeatureProperty.value"@en .
:GenericRoadFeatureProperty rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :GenericRoadFeatureProperty.value;
         owl:allValuesFrom :Any;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#valueReference
:valueReference rdf:type owl:ObjectProperty ;
         skos:definition "A reference to an externally defined value"@en ;
         rdfs:domain :GenericRoadFeatureProperty;
         rdfs:range :PropertyValueReferenceType;
         rdfs:label "valueReference"@en .
:GenericRoadFeatureProperty rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :valueReference;
         owl:allValuesFrom :PropertyValueReferenceType;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl:NatureOfDistanceMeasure
:NatureOfDistanceMeasure a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for contexts of forms of interpreting distance measures"@en ;
       rdfs:label "NatureOfDistanceMeasure"@en .

### http://spec.tn-its.eu/owl/tnits-owl:NatureOfDistanceMeasureCode
:NatureOfDistanceMeasureCode a skos:ConceptScheme ;
         skos:definition "Specifies the available list of enumerations for contexts of forms of interpreting distance measures"@en ;
       dc:isFormatOf :NatureOfDistanceMeasure .

### http://spec.tn-its.eu/owl/tnits-owl:ParkingSiteType
:ParkingSiteType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the types of parking sites"@en ;
       rdfs:label "ParkingSiteType"@en .

### http://spec.tn-its.eu/owl/tnits-owl:ParkingSiteTypeCode
:ParkingSiteTypeCode a skos:ConceptScheme ;
         skos:definition "Specifies the available list of enumerations for the types of parking sites"@en ;
       dc:isFormatOf :ParkingSiteType .

### http://spec.tn-its.eu/owl/tnits-owl:PaymentMethod
:PaymentMethod a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the methods of payment"@en ;
       rdfs:label "PaymentMethod"@en .

### http://spec.tn-its.eu/owl/tnits-owl:PaymentMethodCode
:PaymentMethodCode a skos:ConceptScheme ;
         skos:definition "Specifies the available list of enumerations for the methods of payment"@en ;
       dc:isFormatOf :PaymentMethod .

### http://spec.tn-its.eu/owl/tnits-owl:PropertyValueReferenceType
:PropertyValueReferenceType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "A list of externally defined values "@en ;
       rdfs:label "PropertyValueReferenceType"@en .

### http://spec.tn-its.eu/owl/tnits-owl:PropertyValueReferenceTypeCode
:PropertyValueReferenceTypeCode a skos:ConceptScheme ;
         skos:definition "A list of externally defined values "@en ;
       dc:isFormatOf :PropertyValueReferenceType .

### http://spec.tn-its.eu/owl/tnits-owl:RoadFeature
:RoadFeature a owl:Class ;
       rdfs:subClassOf :tnitsFeature ;
         skos:definition "The base class for any type of road feature. A road feature represents static road data, such as traffic regulations, traffic signs (like speed limits, compulsory signs and warning signs) and road infrastructure (such as charging points). Each road feature must be described by a geo-referenced by providing a point or linear location reference and can be further described by a set of conditions specifying the validity of the road feature (using combinations of conditions such as time restrictions, area, lane, vehicle type, weather, loading or road category)."@en ;
       rdfs:label "RoadFeature"@en .

### http://spec.tn-its.eu/owl/tnits-owl#roadFeatureId
:roadFeatureId rdf:type owl:ObjectProperty ;
         skos:definition "The identity for the road feature. Permanent identifier for the road feature in the database of the data provider. "@en ;
         rdfs:range :Identifier;
         rdfs:label "roadFeatureId"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :roadFeatureId;
         owl:onClass :Identifier;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :roadFeatureId;
         owl:allValuesFrom :Identifier;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#properties
:properties rdf:type owl:ObjectProperty ;
         skos:definition "The property values of interest for the road feature "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :AbstractRoadFeatureProperty;
         rdfs:label "properties"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :properties;
         owl:allValuesFrom :AbstractRoadFeatureProperty;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#RoadFeature.condition
:RoadFeature.condition rdf:type owl:ObjectProperty ;
         skos:definition "Describes the circumstances when the road feature is applicable.  "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :Condition;
         rdfs:label "RoadFeature.condition"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.condition;
         owl:onClass :Condition;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.condition;
         owl:allValuesFrom :Condition;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#RoadFeature.locationReference
:RoadFeature.locationReference rdf:type owl:ObjectProperty ;
         skos:definition "A reference to the location of the road feature either described by either a precoded or a dynamic location referencing method  "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :LocationReference;
         rdfs:label "RoadFeature.locationReference"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.locationReference;
         owl:onClass :LocationReference;
       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.locationReference;
         owl:allValuesFrom :LocationReference;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#validFrom
:validFrom rdf:type owl:DatatypeProperty ;
         skos:definition "The date from which this road feature is valid and in use "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range xsd:date;
         rdf:type owl:FunctionalProperty;
         rdfs:label "validFrom"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :validFrom;
         owl:onDataRange xsd:date;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :validFrom;
         owl:allValuesFrom xsd:date;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#validTo
:validTo rdf:type owl:DatatypeProperty ;
         skos:definition "The date from which this road feature is no longer valid "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range xsd:date;
         rdfs:label "validTo"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :validTo;
         owl:onDataRange xsd:date;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :validTo;
         owl:allValuesFrom xsd:date;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#beginLifespanVersion
:beginLifespanVersion rdf:type owl:DatatypeProperty ;
         skos:definition "Date and time at which this version of the spatial object was inserted or changed in the spatial data set. "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range xsd:dateTime;
         rdf:type owl:FunctionalProperty;
         rdfs:label "beginLifespanVersion"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :beginLifespanVersion;
         owl:onDataRange xsd:dateTime;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :beginLifespanVersion;
         owl:allValuesFrom xsd:dateTime;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#endLifespanVersion
:endLifespanVersion rdf:type owl:DatatypeProperty ;
         skos:definition "Date and time at which this version of the spatial object was superseded or retired in the spatial data set. "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range xsd:dateTime;
         rdfs:label "endLifespanVersion"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :endLifespanVersion;
         owl:onDataRange xsd:dateTime;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :endLifespanVersion;
         owl:allValuesFrom xsd:dateTime;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#RoadFeature.updateInfo
:RoadFeature.updateInfo rdf:type owl:ObjectProperty ;
         skos:definition "Specifies the necessary update information if this is a representation of an update event. "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :UpdateInfo;
         rdfs:label "RoadFeature.updateInfo"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.updateInfo;
         owl:onClass :UpdateInfo;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :RoadFeature.updateInfo;
         owl:allValuesFrom :UpdateInfo;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#source
:source rdf:type owl:ObjectProperty ;
         skos:definition "Identifies the evidence base that has been used by the authorised authority supplying the data. "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :RoadFeatureSource;
         rdf:type owl:FunctionalProperty;
         rdfs:label "source"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :source;
         owl:onClass :RoadFeatureSource;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         ] .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
         owl:onProperty :source;
         owl:allValuesFrom :RoadFeatureSource;
         ] .


### http://spec.tn-its.eu/owl/tnits-owl#RoadFeature.type
:RoadFeature.type rdf:type owl:ObjectProperty ;
         skos:definition "A characterisation identifying the type of the road feature by reference to an item in an external code list "@en ;
         rdfs:domain :RoadFeature;
         rdfs:range :RoadFeatureType;
         rdf:type owl:FunctionalProperty;
         rdfs:label "RoadFeature.type"@en .
:RoadFeature rdfs:subClassOf [ rdf:type owl:Restriction ;
