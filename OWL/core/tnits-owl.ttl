@prefix : <http://spec.tn-its.eu/owl/tnits-owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix lr: <http://www.roadotl.eu/iso19148/def/> .
@prefix sf: <http://www.opengis.net/ont/sf#> .
@prefix gsp: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<http://spec.tn-its.eu/owl/tnits-owl> a owl:Ontology ;
	owl:imports <http://www.w3.org/2004/02/skos/core> ;
	owl:imports <http://schemas.opengis.net/geosparql/1.0/geosparql_vocab_all.rdf> ;
	owl:imports <https://www.roadotl.eu/static/eurotl-ontologies/iso19148_doc/ontology.ttl> ;
	owl:imports <http://def.isotc211.org/iso19115/-1/2014/MetadataInformation.rdf> ;
	dc:creator "TN-ITS" ;
	dc:date "30.09.2021" ;
	dc:description "Ontology for the TN-ITS model for exchange of changes in road attributes"@en ;
	dcterms:title "TN-ITS"@en ;
	dcterms:abstract "Ontology for exchange of changes in road attributes" .

### http://spec.tn-its.eu/owl/tnits-owl#tnits
:tnits a owl:Class ;
         owl:disjointUnionOf ( :tnitsFeature :tnitsCodeList :tnitsEnumeration :tnitsDataType ) ;
         rdfs:label "TN-ITS Root"@en .
### http://spec.tn-its.eu/owl/tnits-owl#Feature
:tnitsFeature a owl:Class ;
         rdfs:subClassOf <http://def.isotc211.org/iso19109/2015/GeneralFeatureModel#AnyFeature> ,
                          :tnits ,
                       gsp:Feature ;
         rdfs:label "TN-ITS Feature"@en .
### http://spec.tn-its.eu/owl/tnits-owl#CodeList
:tnitsCodeList a owl:Class ;
         rdfs:subClassOf :tnits ;
         rdfs:label "TN-ITS Code value"@en .
### http://spec.tn-its.eu/owl/tnits-owl#Enumeration
:tnitsEnumeration a owl:Class ;
         rdfs:subClassOf :tnits ;
         rdfs:label "TN-ITS Enumeration value"@en .
### http://spec.tn-its.eu/owl/tnits-owl#DataType
:tnitsDataType a owl:Class ;
         rdfs:subClassOf :tnits ;
         rdfs:label "TN-ITS Data type"@en .
### http://spec.tn-its.eu/owl/tnits-owl#OPtnits
:OPtnits a owl:ObjectProperty ;
         rdfs:label "TN-ITS Object Properties"@en .
### http://spec.tn-its.eu/owl/tnits-owl#DPtnits
:DPtnits a owl:DatatypeProperty ;
         rdfs:label "TN-ITS Datatype Properties"@en .


:XMLLocationReference a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "A OpenLR location reference encoded using XML according to the OpenLR specification (see bibliography)  "@en ;
       rdfs:label "XMLLocationReference"@en .

:Dataset a owl:Class ;
       rdfs:subClassOf :tnitsFeature ;
         skos:definition "Abstract class that represents any type of dataset that may be exchanged using TN-ITS  "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :metadataProperty ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Metadata ] ;
       rdfs:label "Dataset"@en .
:Dataset owl:disjointUnionOf ( :RoadFeatureDataset :FeedbackDataset  ) ; .
:FeedbackDataset a owl:Class ;
         skos:definition "A dataset containing feedback to the road authorities/road operators from an information provider/map provider in response to data download and integration  "@en ;
       rdfs:subClassOf :Dataset ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :feedbackInfo ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :FeedbackInformation ] ;
       rdfs:label "FeedbackDataset"@en .
:RoadFeatureDataset a owl:Class ;
         skos:definition "A dataset containing road feature with optional update information.  "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :roadFeatures ;
       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadFeature ] ;
       rdfs:subClassOf :Dataset ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :type ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :DatasetType ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :defaultLRM ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass <http://www.roadotl.eu/iso19148/def/LinearReferencingMethod> ] ;
       rdfs:label "RoadFeatureDataset"@en .
:DatasetType a owl:Class ;
       rdfs:subClassOf :tnitsEnumeration ;
         skos:definition "Specifies the different possible types of datasets for road features  "@en ;
       rdfs:label "DatasetType"@en .

### http://spec.tn-its.eu/owl/tnits-owl_DatasetType_Update
:DatasetType_Update rdf:type :DatasetType ;
         skos:definition "A dataset consisting of only incremental updates for road features "@en ;
         rdfs:label "Update"@no .

### http://spec.tn-its.eu/owl/tnits-owl_DatasetType_Snapshot
:DatasetType_Snapshot rdf:type :DatasetType ;
         skos:definition "A snapshot dataset contains information (current status) of all road features in an area, or of all occurrences of one or more specified types of road features"@en ;
         rdfs:label "Snapshot"@no .

:AbstractRoadFeatureProperty a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "An abstract generic (type independent) representation of a road feature property value "@en ;
       rdfs:label "AbstractRoadFeatureProperty"@en .
:AbstractRoadFeatureProperty owl:disjointUnionOf ( :GenericRoadFeatureProperty :RoadSignProperty  ) ; .
:ChargingPointUsageType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the use of electric charging point "@en ;
       rdfs:label "ChargingPointUsageType"@en .
:GasType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for types of propellant gas."@en ;
       rdfs:label "GasType"@en .
:GenericRoadFeatureProperty a owl:Class ;
         skos:definition "A representation of a road feature property value "@en ;
       rdfs:subClassOf :AbstractRoadFeatureProperty ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :type ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadFeaturePropertyType ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :value ;
       owl:allValuesFrom :Any ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :valueReference ;
       owl:allValuesFrom :PropertyValueReferenceType ] ;
       rdfs:label "GenericRoadFeatureProperty"@en .
:NatureOfDistanceMeasure a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for contexts of forms of interpreting distance measures"@en ;
       rdfs:label "NatureOfDistanceMeasure"@en .
:ParkingSiteType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the types of parking sites"@en ;
       rdfs:label "ParkingSiteType"@en .
:PaymentMethod a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the methods of payment"@en ;
       rdfs:label "PaymentMethod"@en .
:PropertyValueReferenceType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "A list of externally defined values "@en ;
       rdfs:label "PropertyValueReferenceType"@en .
:RoadFeature a owl:Class ;
       rdfs:subClassOf :tnitsFeature ;
         skos:definition "The base class for any type of road feature. A road feature represents static road data, such as traffic regulations, traffic signs (like speed limits, compulsory signs and warning signs) and road infrastructure (such as charging points). Each road feature must be described by a geo-referenced by providing a point or linear location reference and can be further described by a set of conditions specifying the validity of the road feature (using combinations of conditions such as time restrictions, area, lane, vehicle type, weather, loading or road category)."@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :properties ;
       owl:allValuesFrom :AbstractRoadFeatureProperty ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :id ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadFeatureId ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :conditionProperty ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Condition ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :locationReferenceProperty ;
       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :LocationReference ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :validFrom ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:date ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :validTo ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:date ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :beginLifespanVersion ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :endLifespanVersion ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :updateInfoProperty ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :UpdateInfo ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :source ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadFeatureSource ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :type ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadFeatureType ] ;
       rdfs:label "RoadFeature"@en .
:RoadFeatureId a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "A class supporting a provider defined identifier for a road feature"@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :providerId ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:string ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :localId ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:string ] ;
       rdfs:label "RoadFeatureId"@en .
:RoadFeaturePropertyType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the various types of road features"@en ;
       rdfs:label "RoadFeaturePropertyType"@en .
:RoadFeatureSource a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the various types source concerning road features"@en ;
       rdfs:label "RoadFeatureSource"@en .
:RoadFeatureType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the various types of road features"@en ;
       rdfs:label "RoadFeatureType"@en .
:RoadSignContent a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Defines one instance of content providing context of the semantics of a road sign "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :value ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Measure ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :text ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:string ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :natureOfDistance ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :NatureOfDistanceMeasure ] ;
       rdfs:label "RoadSignContent"@en .
:RoadSignProperty a owl:Class ;
         skos:definition "A representation of a road feature property value in the case of the road feature being a road sign "@en ;
       rdfs:subClassOf :AbstractRoadFeatureProperty ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :gddCode ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadSignType ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :content ;
       owl:allValuesFrom :RoadSignContent ] ;
       rdfs:label "RoadSignProperty"@en .
:RoadSignType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the road sign types, defined in accordance with ISO 14823"@en ;
       rdfs:label "RoadSignType"@en .
:SiteAccessibilityType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for types of site accessibility"@en ;
       rdfs:label "SiteAccessibilityType"@en .
:TunnelCategory a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the categories of tunnel access"@en ;
       rdfs:label "TunnelCategory"@en .
:UomIdentifier  a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of preferred enumerations for identifiers for units of measure"@en ;
       rdfs:label "UomIdentifier "@en .

:AreaCondition a owl:Class ;
         skos:definition "Specifies area type condition applicability relating to a road feature  "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :areaTypeProperty ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :AreaType ] ;
       rdfs:label "AreaCondition"@en .
:AreaType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the area type condition for a road feature "@en ;
       rdfs:label "AreaType"@en .
:Condition a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Abstract class that specifies a condition for the validity of a certain road feature such as time, weather conditions, vehicle type, vehicle propulsion fuel, vehicle occupancy, vehicle usage, restrictions of the vehicle driver’s licence permissions, vehicle dimensions or weight characteristics."@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :negate ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:boolean ] ;
       rdfs:label "Condition"@en .
:Condition owl:disjointUnionOf ( :TimeCondition :ConditionSet :AreaCondition :VehicleCondition :WeatherCondition :RoadCategoryCondition :LaneCondition  ) ; .
:ConditionSet a owl:Class ;
         skos:definition "Groups a number of conditions into a condition set"@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :conditions ;
       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Condition ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :operator ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :ConditionOperator ] ;
       rdfs:label "ConditionSet"@en .
:LaneCondition a owl:Class ;
         skos:definition "Specifies the applicability of a condition to specific lanes "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :startLane ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:integer ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :laneExtension ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:integer ] ;
       rdfs:label "LaneCondition"@en .
:RoadCategory a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the road category types"@en ;
       rdfs:label "RoadCategory"@en .
:RoadCategoryCondition a owl:Class ;
         skos:definition "Specifies road category condition applicability relating to a road feature "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :roadCategoryProperty ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :RoadCategory ] ;
       rdfs:label "RoadCategoryCondition"@en .
:TimeCondition a owl:Class ;
         skos:definition "Specifies timing applicability relating to a road feature  "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :validityPeriod ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Validity ] ;
       rdfs:label "TimeCondition"@en .
:VehicleCondition a owl:Class ;
         skos:definition "Specifies vehicle type condition applicability relating to a road feature  "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :vehicleCharacteristicsProperty ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :VehicleCharacteristics ] ;
       rdfs:label "VehicleCondition"@en .
:WeatherCondition a owl:Class ;
         skos:definition "Specifies weather type condition applicability relating to a road feature  "@en ;
       rdfs:subClassOf :Condition ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :weatherTypeProperty ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :WeatherType ] ;
       rdfs:label "WeatherCondition"@en .
:WeatherType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Types of weather conditions which affect driving conditions."@en ;
       rdfs:label "WeatherType"@en .
:ConditionOperator a owl:Class ;
       rdfs:subClassOf :tnitsEnumeration ;
         skos:definition "The logical operator to be used in a test of conditions "@en ;
       rdfs:label "ConditionOperator"@en .

### http://spec.tn-its.eu/owl/tnits-owl_ConditionOperator_OR
:ConditionOperator_OR rdf:type :ConditionOperator ;
         skos:definition "The OR logical operator – a logical operation to determine if any conditions in a test are TRUE "@en ;
         rdfs:label "OR"@no .

### http://spec.tn-its.eu/owl/tnits-owl_ConditionOperator_XOR
:ConditionOperator_XOR rdf:type :ConditionOperator ;
         skos:definition "The XOR logical operator - a logical operation that outputs true only when inputs differ (one is true, the other is false). "@en ;
         rdfs:label "XOR"@no .

### http://spec.tn-its.eu/owl/tnits-owl_ConditionOperator_AND
:ConditionOperator_AND rdf:type :ConditionOperator ;
         skos:definition "The AND logical operator – a logical operation to<font color=\"#2f2f2f\"> determine if all conditions in a test are TRUE</font> "@en ;
         rdfs:label "AND"@no .

:FuzzyTimeType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the fuzzy time concepts, which provided the definition of start, end or duration time concepts that support the expression of a non-precise or variable qualified concept, resulting in a non-precise, potentially circumstantial defined time period.  Note: Example roads closed at the start of winter in mountainous areas, which is based on actual experienced weather conditions and therefore is not pre-defined.  "@en ;
       rdfs:label "FuzzyTimeType"@en .
:OverallPeriod a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "<font color=\"#231f20\">A continuous or discontinuous period of validity defined by overall bounding start and end times and the possible intersection of valid periods (potentially recurring) with the complement of exception periods (also potentially recurring). [SOURCE: CEN/TS16157-3:2011]</font> "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :validPeriod ;
       owl:allValuesFrom :Period ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :exceptionPeriod ;
       owl:allValuesFrom :Period ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :overallStartTime ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :overallEndTime ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:label "OverallPeriod"@en .
:Period a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "<font color=\"#231f20\">A continuous time period or a set of discontinuous time periods defined by the intersection of a set of criteria all within an overall delimiting interval. </font> <font color=\"#231f20\">[SOURCE: CEN/TS16157-3:2011]</font> "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :recurringFuzzyTimePeriod ;
       owl:allValuesFrom :FuzzyTimePeriod ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :recurringTimePeriodOfDay ;
       owl:allValuesFrom :TimePeriodOfDay ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :recurringSpecialDay ;
       owl:allValuesFrom :SpecialDay ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :recurringDayWeekMonthPeriod ;
       owl:allValuesFrom :DayWeekMonth ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :startOfPeriod ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :endOfPeriod ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:dateTime ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :periodName ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:string ] ;
       rdfs:label "Period"@en .
:SpecialDay a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Specification of a special type of day, possibly also a public holiday. Can be country or region specific. [SOURCE: DATEX 3.0 Part 7 (proposed EN 161577:2018)] "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :intersectWithApplicableDays ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:boolean ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :specialDayTypeProperty ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :SpecialDayType ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :publicEvent ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onDataRange xsd:string ] ;
       rdfs:label "SpecialDay"@en .
:SpecialDay owl:disjointUnionOf ( :PublicHoliday  ) ; .
:SpecialDayType a owl:Class ;
       rdfs:subClassOf :tnitsCodeList ;
         skos:definition "Specifies the available list of enumerations for the special days"@en ;
       rdfs:label "SpecialDayType"@en .
:TimePeriodOfDay a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Specification of a continuous period of time within a 24 hour period. [SOURCE: CEN/TS16157-3:2011] "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :startTimeOfPeriod ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Time ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :endTimeOfPeriod ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :Time ] ;
       rdfs:label "TimePeriodOfDay"@en .
:Validity a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Specifies the periods of validity that relate to a road feature  "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :validityTimeSpecification ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :OverallPeriod ] ;
       rdfs:label "Validity"@en .
:FuzzyTimePeriod a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
         skos:definition "Specifies the time period with non-precise characteristics  "@en ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :beginOrDuration ;
       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :FuzzyTimeType ] ;
       rdfs:subClassOf [ a owl:Restriction ;
       owl:onProperty :endOrDuration ;
       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
       owl:onClass :FuzzyTimeType ] ;
       rdfs:label "FuzzyTimePeriod"@en .
:DayWeekMonth a owl:Class ;
       rdfs:subClassOf :tnitsDataType ;
