<?xml version="1.0" encoding="UTF-8"?>
<gml:Dictionary xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd"
	xmlns:gsr="http://www.isotc211.org/2005/gsr" xmlns:gss="http://www.isotc211.org/2005/gss"
	xmlns:gts="http://www.isotc211.org/2005/gts" xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="NatureOfDistanceMeasureCode"
	xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
	<gml:description>Specifies the available list of enumerations for contexts of forms of interpreting distance measures</gml:description>
	<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/">NatureOfDistanceMeasureCode</gml:identifier>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="advanceWarning">
			<gml:description>Advance warning notification of a feature with a specified distance to the feature (e.g. Railway crossing in 200 metres) </gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/NatureOfDistanceMeasureCode">advanceWarning</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="featureContinuity">
			<gml:description>Notification that the effect of the Feature continues for the specified distance (e.g. for the next X km).</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/NatureOfDistanceMeasureCode">featureContinuity</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
</gml:Dictionary>
