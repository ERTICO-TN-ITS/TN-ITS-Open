		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="snow">
			<gml:description>Snow, with reduced visibility, and increased skid risk</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/WeatherTypeCode">snow</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="wind">
			<gml:description>Wind conditions on the road</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/WeatherTypeCode">wind</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="wet">
			<gml:description>Road surface is wet</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/WeatherTypeCode">wet</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
