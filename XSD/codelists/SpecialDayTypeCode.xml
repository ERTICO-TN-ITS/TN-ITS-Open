<?xml version="1.0" encoding="UTF-8"?>
<gml:Dictionary xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd"
	xmlns:gsr="http://www.isotc211.org/2005/gsr" xmlns:gss="http://www.isotc211.org/2005/gss"
	xmlns:gts="http://www.isotc211.org/2005/gts" xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="SpecialDayTypeCode"
	xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
	<gml:description>Specifies the available list of enumerations for the special days</gml:description>
	<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/">SpecialDayTypeCode</gml:identifier>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="dayBeforePublicHoliday">
			<gml:description>The day before a declared Public Holiday</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">dayBeforePublicHoliday</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="publicHoliday">
			<gml:description>The day of a declared Public Holiday, which may be national, or regional.</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">publicHoliday</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="dayFollowingPublicHoliday">
			<gml:description>The day following a declared Public Holiday</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">dayFollowingPublicHoliday</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="longWeekendDay">
			<gml:description>
      </gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">longWeekendDay</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="inLieuOfPublicHoliday">
			<gml:description>
      </gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">inLieuOfPublicHoliday</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="schoolDay">
			<gml:description>In the locale a day when schools are open and operating</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">schoolDay</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="schoolHolidays">
			<gml:description>In the locale a day when schools are not open and operating</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">schoolHolidays</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="publicEventDay">
			<gml:description>The day of a declared Public Event, which may be national, or regional.

Note: For example, the day of a Royal Wedding.</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">publicEventDay</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
	<gml:dictionaryEntry>
		<gml:Definition gml:id="other">
			<gml:description>Other than as defined in this enumeration.</gml:description>
			<gml:identifier codeSpace="http://spec.tn-its.eu/codelists/SpecialDayTypeCode">other</gml:identifier>
		</gml:Definition>
	</gml:dictionaryEntry>
</gml:Dictionary>
